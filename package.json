{
  "name": "flamydash-iframe-integration",
  "version": "1.0.0",
  "description": "Complete iframe game integration system for Flamy Dash",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "demo": "node start-demo.js",
    "build": "node build.js",
    "build:cloudflare": "npm run build && echo 'Ready for Cloudflare Pages deployment from /out directory'",
    "test": "node test-integration.js",
    "test:manual": "echo 'Manual testing: Open http://localhost:3000 in browser'",
    "health": "curl -f http://localhost:3000/api/health || echo 'Server not running'",
    "clean": "rm -rf out"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^7.2.0",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "fs-extra": "^11.3.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "game",
    "iframe",
    "unity",
    "webgl",
    "azgame",
    "integration"
  ],
  "author": "Flamy Dash Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
}
